<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC
    "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
    "https://checkstyle.org/dtds/suppressions_1_2.dtd">
<suppressions>
  <suppress files="controller/.*|config/.*"
            checks="MagicNumber"/>
  <suppress files="src/test/.*"
            checks="CyclomaticComplexity|FinalClass|MagicNumber|MethodName"/>
  <suppress files="util/.*"
            checks="CyclomaticComplexity|MagicNumber"/>
  <suppress files="messages.*\.properties"
            checks="LineLength"/>
</suppressions>
